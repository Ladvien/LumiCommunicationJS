#!/bin/sh
rsync -av /Users/user/Dropbox/js-practice/js-ble/*.js /Users/user/ladvien.github.io/javascripts/
rsync -av /Users/user/Dropbox/js-practice/js-ble/*.css /Users/user/ladvien.github.io/css/
rsync -av /Users/user/Dropbox/js-practice/js-ble/*.html /Users/user/ladvien.github.io/_includes/snippets/js-ble

cd /Users/user/Dropbox/js-practice/js-ble
git add .
echo 'Enter the commit message:'
read commitMessage
git commit -m "$commitMessage"
git push
